// Test continue inside switch: should continue the enclosing loop
fn main() {
    var count = 0;
    var i = 0;
    while (i < 5) {
        switch (i) {
            case 2:
                i = i + 1;
                continue;
        }
        count = count + 1;
        i = i + 1;
    }
    return count;  // 4 (skips i=2)
}
