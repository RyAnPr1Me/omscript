// Test array element assignment
fn main() {
    var arr = [10, 20, 30, 40, 50];
    
    // Basic assignment
    arr[0] = 99;
    arr[2] = 77;
    
    // Read back modified values
    var sum = arr[0] + arr[1] + arr[2] + arr[3] + arr[4];
    // 99 + 20 + 77 + 40 + 50 = 286
    // 286 % 256 = 30
    return sum;
}
